    "description": "An auto-completing Laravel Nova search field.",
    "keywords": [
        "laravel",
        "nova",
        "select",
        "search",
        "auto complete",
        "autocomplete",
        "suggest",
        "drop down",
        "dropdown"
    ],
    "authors": [
        {
            "name": "Klemen TuÅ¡ar",
            "email": "techouse@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.6 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Techouse\\SelectAutoComplete\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Techouse\\SelectAutoComplete\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
